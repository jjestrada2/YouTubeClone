                                <form id="form" class="logout" action="/users/logout" method="post" enctype="application/x-www-form-urlencoded">
                                
                                    <button type="submit" value="Logout" class="navigation__link">Logout</button>
                            
                                   
                                </form>
                            </li>
                            {{else}}
                            <li class="navigation__item">
                            <a href="/login" class="navigation__link">Sing In</a>
                            </li>

                            <li class="navigation__item">
